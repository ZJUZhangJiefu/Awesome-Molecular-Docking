    "# distance cutoff: 10 A\n",
    "# if a protain chain has no atoms within 10 A distance of the ligand\n",
    "# then it will be neglected\n",
    "cutoff = 10\n",
    "data_dir = '../data/PDBBind'\n",
    "names = os.listdir(data_dir)\n",
    "io = PDBIO()\n",
    "# parse the PDB files\n",
    "biopython_parser = PDBParser()"
   ]
